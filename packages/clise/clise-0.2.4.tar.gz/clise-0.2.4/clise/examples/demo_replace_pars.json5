
{
	"-main" : "demo.show",
	"x"     : "3",
	"y"     : "{x} there",
	"name"  : "str substitution test with {key}",
},
{
	"-main" : "demo.show",
	"x"     : 3,
	"y"     : 4,
	"name"  : "simple integer input",
},
{
	"-main" : "demo.show",
	"#x"    : "3",
	"#y"    : "{x}*2",
	"name"  : "string to integer conversion with #key",
},
{
	"-main" : "demo.show",
	"-id"   : "this show",
	"#x"    : "2",
	"##y"   : "{x}*2",
	"name"  : "string to float conversion with ##key",
},
{
	"-main" : "demo.show",
	"-id"   : "this show",
	"*x"    : "a,1,2.0",
	"*#y"   : "a,1,2.0",
	"name"  : "string to list conversion with *key or *#key",
},
{
	"-main" : "demo.show",
	"-id"   : "this show",
	"**x"   : "'a':1,'b':2",
	"**y"   : "'a':1,'b':{'c':3,'d':4}",
	"name"  : "string to dict conversion with **key",
},

{
  "version": "0.1.0",
  "configurations": [
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Launch Edge against localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "debug streamlit",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/venv/scripts/streamlit.exe",
      "args": [
        "run",
        "${workspaceFolder}/examples/apps/create_study.py"
      ]
    }
  ]
}

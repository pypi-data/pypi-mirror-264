# 通过创建 globalproposal 来创建一个名为 example-ns 的 namespace
POST http://org1.storage.cc.distribute-compute.cn/api/core/v1/globalproposal
{
  "apiVersion": "core/v1",
  "kind": "globalproposal",
  "metadata": {
    "name": "namespace-proposal",
    "uid": 0
  },
  "spec": {
    "actions": [
      {
        "create": {
          "object": {
            "apiVersion": "core/v1",
            "kind": "namespace",
            "metadata": {
              "name": "example-ns",
              "uid": 0
            },
            "spec": {
              "members": [
                "wx-org1.chainmaker.org",
                "wx-org2.chainmaker.org",
                "wx-org3.chainmaker.org"
              ]
            }
          }
        }
      }
    ]
  }
}


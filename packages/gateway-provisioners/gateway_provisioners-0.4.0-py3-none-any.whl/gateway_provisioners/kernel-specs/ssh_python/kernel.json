{
  "language": "${language}",
  "display_name": "${display_name}",
  "metadata": {
    "debugger": true,
    "kernel_provisioner": {
      "provisioner_name": "distributed-provisioner",
      "config": {}
    }
  },
  "env": {},
  "argv": [
    "python",
    "${install_dir}/scripts/launch_ipykernel.py",
    "--kernel-id",
    "{kernel_id}",
    "--port-range",
    "{port_range}",
    "--response-address",
    "{response_address}",
    "--public-key",
    "{public_key}",
    "--kernel-class-name",
    "${ipykernel_subclass_name}"
  ]
}
